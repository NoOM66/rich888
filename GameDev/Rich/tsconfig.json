{
  "compilerOptions": {
    "target": "ES2019",
    "module": "ESNext",
    "strict": true,
  "moduleResolution": "bundler",
  "allowSyntheticDefaultImports": true,
  "types": ["phaser"],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "outDir": "dist"
  },
  "include": ["src/**/*"]
}
